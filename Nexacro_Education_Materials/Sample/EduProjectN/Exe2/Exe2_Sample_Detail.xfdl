<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="Exe2_Sample_Detail" width="450" height="220" titletext="Exe2_Sample_Detail" onload="Form_onload" border="1px solid #9c9c9c">
