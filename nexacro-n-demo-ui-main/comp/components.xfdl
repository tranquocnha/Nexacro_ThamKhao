    <Script type="xscript5.1"><![CDATA[ 
this.components_onload = function(obj:nexacro.Form,e:nexacro.LoadEventInfo)
{
	this.gfnFormOnLoad();
	
	//Map component (IE10  IE11  Edge  Chrome  Safari  Firefox  Opera & mobile)
	var osVersion = system.osversion;
	if( ((osVersion.indexOf("Android") != -1 || osVersion.indexOf("iOS") != -1) && nexacro._Browser == "Runtime")
	    || (nexacro._Browser == "IE" && nexacro._BrowserVersion >= 10)
		|| nexacro._Browser == "Edge" || nexacro._Browser == "Chrome" || nexacro._Browser == "Safari" || nexacro._Browser == "Firefox")
	{
		this.Div20.set_visible(true);
		this.resetScroll();
	}

	
	
	//Sketch Component
	this.Div16.form.Sketch00.setBrushSize( 3 );
	this.Div16.form.Sketch00.loadSketch( this.ds_sketch.getColumn(0,0) );
	
	
	//WebBrowser Component (운영서버에서 정상작동을 위해서는 https로 호출되어야 함)
	this.Div19.form.WebBrowser00.set_url("https://www.tobesoft.com/");
	
	
	//Graphics Component
	//GraphicsRect
	var objGRect = new nexacro.GraphicsRect();
	this.Div18.form.Graphics00.addChild( "GraphicsRect00", objGRect );
	objGRect.set_x(50);
	objGRect.set_y(50);
	objGRect.set_width(50);
	objGRect.set_height(50);
	objGRect.set_strokepen("2px dashed green");
	
	// GraphicsEllipse
	var objGEllipse = new nexacro.GraphicsEllipse();
	objGEllipse.set_x(150);
	objGEllipse.set_y(75);
 	objGEllipse.set_width(50);
 	objGEllipse.set_height(50);
	objGEllipse.set_strokepen("3px solid gray");	
	
	// GraphicsText
	var objGText = new nexacro.GraphicsText();
	objGText.set_x(200);
	objGText.set_y(50);
	objGText.set_color('red');
	objGText.set_font('14pt/normal Verdana'); 
 	objGText.set_text('tobesoft');
	
	// GraphicsImage
	var objGImage = new nexacro.GraphicsImage();
	objGImage.set_x(180);
	objGImage.set_y(100);	
	objGImage.set_image("url('imagerc::img_WF_sample02.png')");	
	
	// GraphicsLine
	var objGLine = new nexacro.GraphicsLine();
	objGLine.set_x(50);
	objGLine.set_y(120);
	objGLine.set_x2(100);
	objGLine.set_y2(0);
	objGLine.set_strokepen('2px solid red');  
	
	// GraphicsPaths
	var objGPaths = new nexacro.GraphicsPaths();
	objGPaths.set_x(50);
	objGPaths.set_y(120);
	objGPaths.set_strokepen('5px dotted blue');  

	
	var objGPath = new nexacro.GraphicsPath();	
	trace(objGPath.hasSegments());
	objGPaths.addChild( "GraphicsPath00", objGPath );
	objGPath.moveTo(0,0);
	objGPath.arcTo( 50, 50, 50, 100, 0, true, false );
	trace(objGPath.hasSegments());
	
	// GraphicsGroup 
	var objGGroup = new nexacro.GraphicsGroup();
	this.Div18.form.Graphics00.addChild( "GraphicsGroup00", objGGroup );
	objGGroup.addChild("GraphicsEllipse00", objGEllipse);
	objGGroup.addChild("GraphicsText00", objGText);
	objGGroup.addChild("GraphicsImage00", objGImage);
	objGGroup.addChild("GraphicsLine00", objGLine);
	objGGroup.addChild("GraphicsPaths00", objGPaths);
	this.Div18.form.Graphics00.redraw();
	
		
	
	//MAP Component
	this.Div20.form.GoogleMap00.load(false, 37.5240220, 126.9265940, 0, 15);
};

this.GoogleMap00_onload = function(obj:nexacro.GoogleMap,e:nexacro.GoogleMapEventInfo)
{
	var objGoogleMapMaker = new nexacro.GoogleMapMarker();
	objGoogleMapMaker.set_latitude(37.5148693);
	objGoogleMapMaker.set_longitude(127.0607522);
	objGoogleMapMaker.set_text("TOBESOFT");
	objGoogleMapMaker.set_visible(true);
	
	this.Div20.form.GoogleMap00.addItem("MapMarker", objGoogleMapMaker);
};
