<FDL version="2.1">
  <Form id="form" width="920" height="340" onload="form_onload">
    <Layouts>
      <Layout height="340" width="920">
        <Static id="Static00" taborder="1" text="Report" left="20" top="19" width="150" height="30" cssclass="sta_WF_compTitle02" messageid="external.report" fittocontents="width"/>
        <Static id="Static01" taborder="0" text="Ubi Report is a report solution that can be used in a variety of web browsers without installation.  It provides components for easy use in Nexacro Platform and provides automatic reporting using DataSet, Grid, and Pivot components.&#13;&#10;For related inquiries, please contact &lt;l v='mailto:ubireport@ubireport.com'&gt;ubireport@ubireport.com&lt;/l&gt; or &lt;l v='http://www.ubireport.com/contact/contact.jsp'&gt;http://www.ubireport.com/contact/contact.jsp&lt;/l&gt;" left="20" top="Static00:2" height="60" cssclass="sta_WF_txt15px" wordWrap="english" fittocontents="height" messageid="external.report.desc" usedecorate="true" right="20" width="880"/>
        <Static id="Static02" taborder="2" text="Auto (Grid)" left="20" top="Static01:10" width="150" height="30" cssclass="sta_WF_compTitle02" messageid="external.report.autogrid" fittocontents="width"/>
        <Static id="Static03" taborder="3" text="A feature that automatically interprets the grid to create a report. Click the radio button to change the format of the grid and click the 'Preview' button. Ubi Report automatically create report files without having to create report files by format.&#13;&#10;Drag &amp; drop the grid heads to change order or click the 'Preview' button after you click the head to sort the data. The report is created as it appears on the screen." left="20" top="Static02:2" height="80" cssclass="sta_WF_txt15px" wordWrap="english" fittocontents="height" messageid="external.report.autogrid.desc" usedecorate="true" width="880"/>
        <Static id="Static04" taborder="4" text="Auto (Pivot)" left="20" top="Static03:10" width="150" height="30" cssclass="sta_WF_compTitle02" messageid="external.report.autopivot" fittocontents="width"/>
        <Static id="Static05" taborder="5" text="A feature that automatically interprets pivot grids to create reports. Try changing the Pivot's row/column, alignment, and filter conditions and clicking the Preview button.  The report is created as it appears on the screen." left="20" top="Static04:2" height="40" cssclass="sta_WF_txt15px" wordWrap="english" fittocontents="height" messageid="external.report.autopivot.desc" usedecorate="true" width="880"/>
      </Layout>
    </Layouts>
