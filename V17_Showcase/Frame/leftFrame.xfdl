    <Script type="xscript5.1"><![CDATA[/*
 * 01. Work scope  : frame
 * 02. Screen name  : leftFrame.xfdl
 * 03. Screen explanation : Frame menu screen
 * 04. Creation date  : 2014.02.21
 * 05. Created by  : sian
 * 06. Revision history :
 *********************************************************************
 *     Modification date          Name    Reason
 *********************************************************************
 *
 *********************************************************************
 */   

/************************************************************************************************
 * 1. Common library
 ************************************************************************************************/

/************************************************************************************************
 * 2. Veriable declaration
 ************************************************************************************************/
 var objApp = nexacro.getApplication();    
 
/************************************************************************************************
 * 3. Event processing
 ************************************************************************************************/
//Form onload 이벤트
this.leftFrame_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	gv_topFrame.form.btnLanguage.set_tooltiptext('Changing menu name in Korean');
	
	//대메뉴 변경함수 호출
	this.fn_changeTopMenu(7000);
}  

//메뉴 onclick 이벤트
this.menu_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	var objParent = obj.parent.parent;
	var nDivHeight = 40;
	if(obj.level < 2)
	{
		if(nDivHeight < objParent.height)
		{
			//expand -> collaspan
			objParent.setOffsetHeight(nDivHeight);
		}
		else
		{	//collaspan -> expand
			objParent.setOffsetHeight(objParent.subHeight);
		}
		
		this.div_Menu.form.resetScroll();
	}
	else
	{
		this.fn_openMenuRow(obj.row);
	}
}

//Index 버튼 onclick 이벤트
this.btn_index_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.div_search.set_visible(false);
	this.div_Menu.set_visible(true);
	
	obj.set_cssclass("btn_LF_index_Selected");
	this.btn_searchResult.set_cssclass("btn_LF_searchResult");
};

//Search Result 버튼 onclick 이벤트
this.btn_searchResult_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	this.div_Menu.set_visible(false);
	this.div_search.set_visible(true);
	
	obj.set_cssclass("btn_LF_searchResult_Selected");
	this.btn_index.set_cssclass("btn_LF_index");
};

//검색 Edit onkeydown 이벤트
this.div_left_edt_search_onkeydown = function(obj:Edit, e:nexacro.KeyEventInfo)
{
	//검색 Edit에서 엔터 입력시 검색 실행
	if(e.keycode == 13){
		
		//검색 함수 호출
		this.fn_find();
	}
}

//검색 버튼 onclick 이벤트
this.btn_search_onclick = function(obj:Button,  e:nexacro.ClickEventInfo)
{
	//검색 함수 호출
	this.fn_find();
}

//Expand/Collapse 버튼 onclick 이벤트
this.st_all_expand_onclick = function(obj:nexacro.Static,e:nexacro.ClickEventInfo)
{
	//Expand/Callapse 값 변경
	if(!obj.isExpand)obj.isExpand = true;
	else obj.isExpand = false;
	
	
	
	var objDiv = this.div_Menu.form.components;
	var objSubDiv;
	var nDivHeight = 40;
	
	for(var i=0; i<objDiv.length; i++)
	{
		//접기
		if(!obj.isExpand)
		{
			this.st_all_expand.set_text("Expand All");
			nDivHeight = 40;
		}
		//펼치기
		else
		{
			this.st_all_expand.set_text("Collapse All");
			nDivHeight = objDiv[i].subHeight;
		}
		
		objDiv[i].set_height(nDivHeight);
	}
	
	this.div_Menu.form.resetScroll();
};

/************************************************************************************************
 * 4. Function processing
 ************************************************************************************************/
this.fn_openMenuRow = function(row)
{
	//Left메뉴 
	gv_vFrameSet1.set_separatesize("45,*");

	var sMenuId = this.ds_Menu.getColumn(row, objApp.gv_MenuIdCol);
	
	//미구현 화면 에외 처리
	if(sMenuId == "700082")return;
		
	var strArgs = "";
	
	//언어별 파라미터 설정
	if( objApp.gv_locale == "KO"){
		strArgs += ("MenuNm="   + this.ds_Menu.getColumn(row,"MENU_NAME_KO").trim());
		strArgs += ("||DescUrl=" + this.ds_Menu.getColumn(row,"Desc_URL_EN"));
		strArgs += ("||DescUrl_ko=" + this.ds_Menu.getColumn(row,"Desc_URL_KO"));		
	}else{
		strArgs += ("MenuNm="   + this.ds_Menu.getColumn(row,"MENU_NAME").trim());
		strArgs += ("||DescUrl=" + this.ds_Menu.getColumn(row,"Desc_URL_EN"));
		strArgs += ("||DescUrl_ko=" + this.ds_Menu.getColumn(row,"Desc_URL_KO"));
	}
	
	//trace("strArgs :: " + strArgs );
	//메뉴실행 함수 호출
	lib_Form.gfn_openMenu(sMenuId, "", strArgs);
}

//검색 함수
this.fn_find = function()
{
	//검색어 가져오기
	var str = this.edt_search.value;
	
	//검색어가 없을 경우 전체 검색
	if(this.edt_search.value == "undefined" && this.edt_search.value == "")
	{
		this.ds_Search_Menu.filter("");
	}
	//검색어가 있을 경우 영문/한국어 필드에서 검색
	else{
		this.ds_Search_Menu.filter("");
		this.ds_Search_Menu.filter("((MENU_NAME).indexOf(\""+str+"\") >= 0) || (MENU_NAME_KO).indexOf(\""+str+"\") >= 0"); 

		//검색 결과가 없을 경우
		if(this.ds_Search_Menu.rowcount == 0)
			this.div_search.set_text("There is no search result.");
	}
	
	//메뉴 영역 숨기기
	this.div_Menu.set_visible(false);
	
	//검색 영역 보이기
	this.div_search.set_visible(true);
	
	//검색 결과 그리기 함수 호출
	this.fn_drawFindData();
}

//언어 변경 처리 함수
this.fn_setLanguage = function()
{
	//한국어로 변경
	if(objApp.gv_locale == "KO")
	{    
		gv_topFrame.form.btnLanguage.set_tooltiptext('Changing menu name in English');
		objApp.gv_MenuNameCol="MENU_NAME_KO";
		objApp.gds_Nations.filter("Nations == 'KO'");
		objApp.gds_Nations.set_rowposition(0);
	} 
	//영어로 변경
	else {
		gv_topFrame.form.btnLanguage.set_tooltiptext('Changing menu name in Korean');
		objApp.gv_MenuNameCol="MENU_NAME";
		objApp.gds_Nations.filter("Nations == 'EN'");
		objApp.gds_Nations.set_rowposition(0);
	}
	
	//메뉴영역 언어변경
	var compDiv = this.div_Menu.form.components;
	var subDiv, compBtn;
	var objBtn;
	var objHeight;
	
	for(var i=0; i<compDiv.length; i++)
	{
		subDiv = compDiv[i];
		compBtn = subDiv.form.components;
		
		for(var b=0; b<compBtn.length; b++)
		{
			objBtn = compBtn[b];
			
			if(objApp.gv_locale == "KO")
			{
				objBtn.set_text(objBtn.ko);
				objBtn.setOffsetHeight(objBtn.koH);
			}
			else
			{
				objBtn.set_text(objBtn.en);
				objBtn.setOffsetHeight(objBtn.enH);
			}
		}
		
		//Position Base가 동작되도록 resetScroll 실행
		subDiv.form.resetScroll();
		subDiv.subHeight = objBtn.getOffsetTop() + objBtn.getOffsetHeight();
		subDiv.setOffsetHeight(subDiv.subHeight);
	}
	this.div_Menu.form.resetScroll();
	
	//검색영역 언어변경
	compDiv = this.div_search.form.components;
	subDiv = null, compBtn = null;
	for(var i=0; i<compDiv.length; i++)
	{
		subDiv = compDiv[i];
		compBtn = subDiv.form.components;
		
		for(var b=0; b<compBtn.length; b++)
		{
			if(objApp.gv_locale == "KO")
			{
				compBtn[b].set_text(compBtn[b].ko);
				compBtn[b].setOffsetHeight(compBtn[b].koH);
			}
			else
			{
				compBtn[b].set_text(compBtn[b].en);
				compBtn[b].setOffsetHeight(compBtn[b].enH);
			}
		}
		//Position Base가 동작되도록 resetScroll 실행
		subDiv.form.resetScroll();
		subDiv.setOffsetHeight(objBtn.getOffsetTop() + objBtn.getOffsetHeight());
	}

	var idx = 0;
	
	//mainPotal화면이 있을 경우 TabFrame 리스트에서 예외처리
	if( gv_workFrame.frames.length > 1 && gv_workFrame.frames[0].name == "MainForm"){
		idx = 1;
		
		//mainPotal 언어변경 처리를 위한 resetScroll();
		gv_MainForm.form.resetScroll();
	}
	
	//TabFrame을 사용하지 않을 경우
	if( gv_vFrameSet1.separatesize == "0,*" ) return;
	
	//TabFrame영역의 언어 변경
	for(var i=idx; i<gv_workFrame.frames.length; i++)
	{
		//TabButton Text 변경 함수 호출
		gv_mdiTabFrame.form.fn_ChangeText(gv_workFrame.frames[i].name, i);
	}
}

//대메뉴 변경 함수
this.fn_changeTopMenu = function (topMenuId)
{
	var filterExpr = objApp.gv_MenuGIdCol + "==" + nexacro.wrapQuote(topMenuId) + "&&" + objApp.gv_MenuLvlCol + "!=0";
	objApp.gds_Menu.set_enableevent(false);
	this.ds_Menu.set_enableevent(false);
	this.ds_Search_Menu.set_enableevent(false);

	objApp.gds_Menu.filter(filterExpr);

	this.ds_Menu.copyData(objApp.gds_Menu, true);
	this.ds_Search_Menu.copyData(objApp.gds_Menu, true);

	this.ds_Search_Menu.filter("   ");
	objApp.gds_Menu.filter();

	objApp.gds_Menu.set_enableevent(true);
	this.ds_Menu.set_enableevent(true);
	this.ds_Search_Menu.set_enableevent(true);
	
	//메뉴 영역 그리기 함수 호출
	this.fn_drawMenuData();
}

//메뉴 영역 그리기 함수
this.fn_drawMenuData = function ()
{
	var objDiv = this.div_Menu;
		
	objDiv.set_visible(false);
	objDiv.set_enableevent(false);	
	this.ds_Menu.set_enableevent(false);
	
	//기존 내용 초기화
	this.fn_clearDiv(objDiv);
	
	var objCreateDiv,objCreateDivId;
	var nDivLeft=0, nDivTop=0, nDivWidth=258, nDivHeight=40;
	
	var objButton,objBtnId;
	var sBtnIdPrev;
	var nButtonLeft=0, nButtonTop=0;
	var nButtonWidth=nDivWidth;
	var nButtonHeight=nButtonHeightKo=nButtonHeightEn=nDivHeight;
	var sButtonClass;
	var mLevel, mGroup, mColId, mNameCol;
	var mCount;
	var exObj;
	var nyText = 0, nyText2 = 0;
	var nRowCount = this.ds_Menu.rowcount;
	
	for(var i=0; i<nRowCount; i++)
	{
		mLevel = this.ds_Menu.getColumn(i,objApp.gv_MenuLvlCol);
		mGroup = this.ds_Menu.getColumn(i,objApp.gv_MenuGIdCol);
		mColId = this.ds_Menu.getColumn(i,objApp.gv_MenuIdCol);
		mNameCol = this.ds_Menu.getColumn(i,"MENU_NAME");
		mNameColKo = this.ds_Menu.getColumn(i,"MENU_NAME_KO");
		
		if(mLevel > 0)
		{
			//중메뉴일 경우
			if(mLevel < 2)
			{
				//중메뉴 그룹을 위한 Div 생성
				mCount = 0;
				exObj = objCreateDiv;
				objCreateDiv = new Div();
				objCreateDivId = "div_sub_"+mColId;
				objCreateDiv.init(objCreateDivId,nDivLeft,nDivTop,nDivWidth,nDivHeight,null,null);
				
				objCreateDiv.set_formscrolltype("none");
				objDiv.addChild(objCreateDivId,objCreateDiv);
				objCreateDiv.show();
				
				if(!lib_Form.gfn_isNull(exObj))
				{
					objCreateDiv.set_top(exObj.name+":0px");
				}
				
				//중메뉴 버튼 설정
				nButtonTop=0;
				objBtnId	= "btn_menu_" + mColId;
				sBtnIdPrev = objBtnId
				sButtonClass = "btn_LF_1depth";
				nButtonHeight = nDivHeight;
				nDivTop += nDivHeight;
			}
			//소메뉴일 경우
			else
			{
				//소메뉴 버튼 설정
				mCount ++;
				//이전 버튼의 하단에 연결되도록 Top Position 설정
				nButtonTop = sBtnIdPrev +":0px";
				objBtnId	= "btn_menu_" + mColId;
				sBtnIdPrev = objBtnId;
				sButtonClass = "btn_LF_2depth";
						
				//Text가 길 경우 WordWarp 처리하며 WorkWarp 처리시
				//버튼의 Height를 다르게 처리함
				nButtonHeightEn = this.fn_getButtonSize(mNameCol,'normal 14px/normal "맑은 고딕"',nButtonWidth-53,"english");
				nButtonHeightKo = this.fn_getButtonSize(mNameColKo,'normal 14px/normal "맑은 고딕"',nButtonWidth-53,"english");
				
				if(objApp.gv_locale == "KO")nButtonHeight = nButtonHeightKo;
				else nButtonHeight = nButtonHeightEn;
			}
			
			//설정된 버튼 생성
			objButton	= new Button();
			objButton.init(objBtnId,nButtonLeft,nButtonTop,nButtonWidth,nButtonHeight,null,null);
			
			if(objApp.gv_locale == "KO")objButton.set_text(mNameColKo);
			else objButton.set_text(mNameCol);
			
			objButton.set_cssclass(sButtonClass);
			objButton.set_wordWrap("english");
			
			//메뉴레벨
			objButton.level = mLevel;
			
			//ds_menu기준 row Index
			objButton.row = i;
			
			//한글명
			objButton.ko = mNameColKo;
			
			//영문명
			objButton.en = mNameCol;
			
			//한글명 Height
			objButton.koH = nButtonHeightKo;
			
			//영문명 Height
			objButton.enH = nButtonHeightEn;
			
			objButton.addEventHandler("onclick",this.menu_onclick, this);
			
			objCreateDiv.addChild(objBtnId,objButton);
			objButton.show();
		}
		
		//중메뉴 그룹을 위한 Div Height 재정의
		objCreateDiv.subHeight = objButton.getOffsetTop() + objButton.getOffsetHeight();
	}
	
	this.div_Menu.form.resetScroll();
	this.ds_Menu.set_enableevent(true);
	objDiv.set_enableevent(true);
	objDiv.set_visible(true);
}

//버튼 사이즈 구하기 함수
this.fn_getButtonSize = function(sText, sFont, nMaxWidth, wordWarp)
{
	var nButtonHeight;
	var nButtonHeight1 = 36; //1줄
	var nButtonHeight2 = 50; //2줄
	var nButtonHeight3 = 64; //3줄
	
	//nyText값은 브라우저마다 조금씩 다름
	//브라우저 마다 적용된 Font의 Text 표현 사이즈가 다름
	//ie			runtime			chrome
	//1줄 : 19		1줄 : 16		1줄 : 18
	//2줄 : 37		2줄 : 32		2줄 : 36
	//3줄 : 56		3줄 : 48		3줄 : 54
	nyText = nexacro.getTextSize(sText, sFont, nMaxWidth, wordWarp).ny;
		
	if(nyText > 45)nButtonHeight = nButtonHeight3;
	else if(nyText > 30)nButtonHeight = nButtonHeight2;
	else nButtonHeight =  nButtonHeight1;
	
	return nButtonHeight;
}


//검색 결과 그리기 함수
this.fn_drawFindData = function ()
{
	var objDiv = this.div_search;
		
	objDiv.set_visible(false);	
	objDiv.set_enableevent(false);	
	this.ds_Search_Menu.set_enableevent(false);
			
	//Div 내용 초기화 함수 호출
	this.fn_clearDiv(objDiv);
	
	var objCreateDiv,objCreateDivId;
	var nDivLeft=0, nDivTop=0, nDivWidth=258, nDivHeight=40;
	
	var objButton,objBtnId;
	var sBtnIdPrev;
	var nButtonLeft=0, nButtonTop=0;
	var nButtonWidth=nDivWidth;
	var nButtonHeight=nButtonHeightKo=nButtonHeightEn=nDivHeight;
	var sButtonClass;
	var nButtonHeight1 = 36, nButtonHeight2 = 50, nButtonHeight3 = 64;
	var mLevel, mGroup, mColId, mNameCol;
	var mCount;
	var exObj;
	var nyText = 0, nyText2 = 0;
	var nFRow;
	var nRowCount = this.ds_Search_Menu.rowcount;
	var nSubHeight = 0;
	
	//중메뉴 그룹 생성
	objCreateDiv = new Div();
	objCreateDivId = "div_sub";
	objCreateDiv.init(objCreateDivId,nDivLeft,nDivTop,nDivWidth,nDivHeight,null,null);
	
	objCreateDiv.set_formscrolltype("none");
	objDiv.addChild(objCreateDivId,objCreateDiv);
	objCreateDiv.show();
	
	
	for(var i=0; i<nRowCount; i++)
	{
		mLevel = this.ds_Search_Menu.getColumn(i,objApp.gv_MenuLvlCol);
		mGroup = this.ds_Search_Menu.getColumn(i,objApp.gv_MenuGIdCol);
		mColId = this.ds_Search_Menu.getColumn(i,objApp.gv_MenuIdCol);
		mNameCol = this.ds_Search_Menu.getColumn(i,"MENU_NAME");
		mNameColKo = this.ds_Search_Menu.getColumn(i,"MENU_NAME_KO");
		nFRow = this.ds_Menu.findRow(objApp.gv_MenuIdCol, mColId);
		
		//소메뉴일 경우
		if(mLevel > 1)
		{
			//소메뉴 버튼 설정
			objButton	= new Button();
			objBtnId	= "btn_menu_" + mColId;
			sButtonClass = "btn_LF_2depth";
			
			//Text가 길 경우 WordWarp 처리하며 WorkWarp 처리시
			//버튼의 Height를 다르게 처리함
			nButtonHeightEn = this.fn_getButtonSize(mNameCol,'normal 14px/normal "맑은 고딕"',nButtonWidth-53,"english");
			nButtonHeightKo = this.fn_getButtonSize(mNameColKo,'normal 14px/normal "맑은 고딕"',nButtonWidth-53,"english");
			
			if(objApp.gv_locale == "KO")nButtonHeight = nButtonHeightKo;
			else nButtonHeight = nButtonHeightEn;
			
			//설정된 버튼 생성
			objButton	= new Button();
			objBtnId	= "btn_menu_" + mColId;
			objButton.init(objBtnId,nButtonLeft,nButtonTop,nButtonWidth,nButtonHeight,null,null);
			
			if(objApp.gv_locale=="KO")
			{
				objButton.set_text(mNameColKo);
			}else{
				objButton.set_text(mNameCol);
			}
			
			objButton.set_cssclass(sButtonClass);
			objButton.set_wordWrap("english");
			
			//메뉴레벨
			objButton.level = mLevel;
			
			//ds_menu기준 row Index
			objButton.row = nFRow;
			
			//한글명
			objButton.ko = mNameColKo;
			
			//영문명
			objButton.en = mNameCol;
			
			//한글명 Height
			objButton.koH = nButtonHeightKo;
			
			//영문명 Height
			objButton.enH = nButtonHeightEn;
			
			objButton.addEventHandler("onclick",this.menu_onclick, this);
			
			objCreateDiv.addChild(objBtnId,objButton);
			objButton.show();
			
			//다음 생성될 버튼의 Top Position을 현재 버튼 하단에 연결되도록 설정
			nButtonTop = objBtnId+":0px";
		}
	}
	
	//중메뉴 그룹을 위한 Div Height 재정의
	if(nRowCount>0)nSubHeight = objButton.getOffsetTop() + objButton.getOffsetHeight();
	
	objCreateDiv.subHeight = nSubHeight;
	
	//중메뉴 그룹 Div Height 설정
	objCreateDiv.setOffsetHeight(objCreateDiv.subHeight);
	
	objDiv.form.resetScroll();
	this.ds_Search_Menu.set_enableevent(true);
	objDiv.set_enableevent(true);
	objDiv.set_visible(true);	
}

//Div 내용 초기화 함수
this.fn_clearDiv = function(objDiv)
{
	var objDivComp = objDiv.form.components;
	var objSubChild;
	var objRemoveChild;
	
	var nCompCount = objDivComp.length;
	var nSubCompCount;
	
	if(nCompCount > 0)
	{				
		for(var c=nCompCount-1; c>-1; c--)
		{
			objSubChild = objDivComp[c].form.components;
			nSubCompCount = objSubChild.length;
			for(var h=nSubCompCount-1; h>-1; h--)
			{
				objRemoveChild = objDiv.removeChild(objSubChild[h]);
				objSubChild[h].destroy();
			}
			
			objDiv.removeChild(objDivComp[c]);
			objDivComp[c].destroy();
		}
	}
}

//좌측메뉴 접기/펼치기 함수
