/***********************************************************************
 * Script Include
 ************************************************************************/
include "lib::lib_Form.xjs"
/***********************************************************************
 * Menu function declaration
 ************************************************************************/
/***********************************************************************
 * Event processing specification
 ************************************************************************/
this.form_onload = function (obj:Form, e:TOBE.LoadEventInfo)
{
	// Common functionality processing on Form Load
	this.gfn_FormOnLoad(obj);
	// Grid, Combo Box Data receive
	this.fn_getComboData();
};

// Form beforeclose Event processing
// Before closing the form, here is pop-up creation to ask if there is modified data, pop-up creating
this.form_onbeforeclose = function (obj:Form, e:TOBE.CloseEventInfo)
{
	if (this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		var retValue = this.gfn_Confirm("msg.before.movepage");
		if (retValue == false) 
		{
			return true;
		}

		this.fn_Save();
		return false;
	}
};

// Event processing on search button click
this.btn_Search_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_PageSearch();
};


// Event processing on add Menu button click
this.btn_Insert_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_Insert();
};

// Event processing on Menu delete button click
this.btn_Delete_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_Delete();
};

// Event processing on Save button click
this.btn_Save_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	if (!this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		this.gfn_Alert("msg.save.nodata");
		return;
	}

	var retValue = this.gfn_Confirm("msg.before.save");
	if (retValue == false) 
	{
		return;
	}

	this.fn_Save();
};

// Event processing on change button click
this.btn_Apply_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_Apply();
};

this.btn_Cancel_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_Cancel();
};

this.btn_moveUp_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_moveUp();
	this.grd_menu.redraw();
};

this.btn_moveDown_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_moveDown();
	this.grd_menu.redraw();
};

this.btn_moveLeft_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_moveLeft();
	this.grd_menu.redraw();
};

this.btn_moveRight_onclick = function (obj:Button, e:TOBE.ClickEventInfo)
{
	this.fn_moveRight();
	this.grd_menu.redraw();
};

// After input value and pressing enter key , this event occurred
this.edt_searchKeyword_onkeydown = function (obj:Edit, e:TOBE.KeyEventInfo)
{
	if (e.keycode == 13) 
	{
		this.fn_PageSearch();
	}
};

// Verifing for the same exists when entering the ID
this.ds_Menu_cancolumnchange = function (obj:Dataset, e:TOBE.DSColChangeEventInfo)
{
	if (e.columnid == "MENU_ID" && !this.gfn_isNull(e.newvalue)) 
	{
		var bRet = this.fn_isExistCode(e.newvalue);
		if (bRet == true) 
		{
			this.gfn_Alert("msg.err.cancolumnchange", "error");
			return false;
		}
	}
};

this.ds_Menu_onrowposchanged = function (obj:Dataset, e:TOBE.DSRowPosChangeEventInfo)
{
	this.edt_ID.set_readonly(!(obj.getRowType(e.newrow) == Dataset.ROWTYPE_INSERT));
};

/***********************************************************************
 * User defined function specification
 ************************************************************************/
this.fn_getComboData = function ()
{
	return;
	var sArgument = "";
	var sSvcID = "getCommCodeList";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getCodeList'" + " ";
	sArgument += "querySetCount='4'" + " ";
	sArgument += "querySet1_Code='30000'" + " ";
	sArgument += "querySet2_Code='10000'" + " ";
	sArgument += "querySet3_Code='20000'" + " ";
	sArgument += "querySet4_Code='50000'";

	var sInDatasets = "";
	var sOutDatasets = "ds_Type=querySet1 ds_ExType=querySet2 ds_PageType=querySet3 ds_Stat=querySet4";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
};

// Add
this.fn_Insert = function ()
{
	var nCurRow = this.ds_Menu.rowposition;
	var nRow = this.ds_Menu.insertRow(nCurRow + 1);

	this.ds_Menu.setColumn(nRow, "CREATE_DT", this.gfn_Today());
	this.ds_Menu.setColumn(nRow, "UPDATE_DT", this.gfn_Today());
	this.ds_Menu.setColumn(nRow, "MENU_SEQ", nRow);
	this.ds_Menu.setColumn(nRow, "MENU_LEVEL", 0);
	this.ds_Menu.setColumn(nRow, "MENU_OPTP", "MAIN");
	this.ds_Menu.setColumn(nRow, "MENU_STAT", "VIEW");

	if (nCurRow == -1) 
	{
		return;
	}

	switch (this.cmb_Level.value) 
	{
		case "1":
			this.ds_Menu.setColumn(nRow, "MENU_LEVEL", this.ds_Menu.getColumn(nCurRow, "MENU_LEVEL") + 1);
			break;
		case "2":
			this.ds_Menu.setColumn(nRow, "MENU_LEVEL", this.ds_Menu.getColumn(nCurRow, "MENU_LEVEL"));
			break;
		case "3":
			break;
	}

	var nTopRow = this.grd_menu.getTreeParentRow(nRow);
	if (nTopRow > -1) 
	{
		this.ds_Menu.setColumn(nRow, "MENU_GRP", this.ds_Menu.getColumn(nTopRow, "MENU_GRP"));
		this.ds_Menu.setColumn(nRow, "MENU_TOP", this.ds_Menu.getColumn(nTopRow, "MENU_ID"));
	}
};

// Menu delete
this.fn_Delete = function ()
{
	var nRow = this.ds_Menu.rowposition;
	this.ds_Menu.deleteRow(nRow);

};

// Changing the selected screen on the menu. 
this.fn_Apply = function ()
{
	if (this.ds_Page.rowposition < 0) 
	{
		this.gfn_Alert("msg.err.noselect", "error", "domain.page");
		return;
	}

	var sPageId = this.ds_Page.getColumn(this.ds_Page.rowposition, "PAGE_ID");
	var sPageName = this.ds_Page.getColumn(this.ds_Page.rowposition, "PAGE_NAME");
	var sPageUrl = this.ds_Page.getColumn(this.ds_Page.rowposition, "PAGE_URL");

	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_ID", sPageId);
	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_NAME", sPageName);
	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_URL", sPageUrl);

	if (this.gfn_isNull(this.ds_Menu.getColumn(this.ds_Menu.rowposition, "MENU_NAME"))) 
	{
		this.ds_Menu.setColumn(this.ds_Menu.rowposition, "MENU_NAME", sPageName);
	}
};

this.fn_Cancel = function ()
{
	if (this.ds_Menu.rowposition < 0) 
	{
		return;
	}

	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_ID", "");
	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_NAME", "");
	this.ds_Menu.setColumn(this.ds_Menu.rowposition, "PAGE_URL", "");
};

// Menu search
this.fn_Search = function ()
{
	return;
	if (this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		var retValue = this.gfn_Confirm("msg.before.search");
		if (retValue == false) 
		{
			return;
		}
	}

	var sArgument = "";
	var sSvcID = "getListMenu";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getList'" + " ";
	sArgument += "querySetCount='1'" + " ";
	sArgument += "querySet1='findMenuList'";

	var sInDatasets = "";
	var sOutDatasets = "ds_Menu=querySet1";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
};

// Screen search
this.fn_PageSearch = function ()
{
	return;
	this.gfn_updateToDataset();

	var sArgument = "";
	var sSvcID = "getListPage";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getList'" + " ";
	sArgument += "querySetCount='1'" + " ";
	sArgument += "querySet1='findPageList'";

	var sInDatasets = "querySet1=ds_Search";
	var sOutDatasets = "ds_Page=querySet1";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
};

this.fn_moveUp = function ()
{
	if (this.ds_Menu.rowposition <= 0) 
	{
		return;
	}

	var nCurRow = this.ds_Menu.rowposition;
	nCurRow = this.ds_Menu.moveRow(nCurRow, nCurRow - 1);
	this.ds_Menu.setColumn(nCurRow, "MENU_SEQ", nCurRow);

	if (nCurRow == 0) 
	{
		this.ds_Menu.setColumn(nCurRow, "MENU_LEVEL", 0);
		this.ds_Menu.setColumn(nCurRow, "MENU_TOP", "");
		this.ds_Menu.setColumn(nCurRow, "MENU_GRP", this.ds_Menu.getColumn(nCurRow, "MENU_ID"));
	}

	var nTopRow = this.grd_menu.getTreeParentRow(nCurRow);
	if (nTopRow > -1) 
	{
		this.ds_Menu.setColumn(nCurRow, "MENU_LEVEL", this.ds_Menu.getColumn(nTopRow, "MENU_LEVEL") + 1);
	}
};

this.fn_moveDown = function ()
{
	if (this.ds_Menu.rowposition < 0) 
	{
		return;
	}

	var nCurRow = this.ds_Menu.rowposition;
	nCurRow = this.ds_Menu.moveRow(nCurRow, nCurRow + 1);

	this.ds_Menu.setColumn(nCurRow, "MENU_SEQ", nCurRow);
};

this.fn_moveLeft = function ()
{
	if (this.ds_Menu.rowposition < 0) 
	{
		return;
	}

	var nCurRow = this.ds_Menu.rowposition;
	var nLevel = this.ds_Menu.getColumn(nCurRow, "MENU_LEVEL");

	if (nLevel <= 0) 
	{
		return;
	}

	this.ds_Menu.setColumn(nCurRow, "MENU_LEVEL", nLevel - 1);
};

this.fn_moveRight = function ()
{
	if (this.ds_Menu.rowposition < 0) 
	{
		return;
	}

	var nCurRow = this.ds_Menu.rowposition;
	var nLevel = this.ds_Menu.getColumn(nCurRow, "MENU_LEVEL");

	this.ds_Menu.setColumn(nCurRow, "MENU_LEVEL", nLevel + 1);
};

// Processing for verifying the same menu id exists or not
this.fn_isExistCode = function (strMenuID)
{
	return;
	var nRow = this.ds_Menu.findRow("MENU_ID", strMenuID);
	if (nRow >= 0) 
	{
		return true;
	}

	var sArgument = "";
	var sSvcID = "getCheckMenu";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getList'" + " ";
	sArgument += "querySetCount='1'" + " ";
	sArgument += "MENU_ID=" + TOBE.wrapQuote(strMenuID) + " ";
	sArgument += "querySet1='findMenu'";

	var sInDatasets = "";
	var sOutDatasets = "ds_MenuKey=querySet1";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc, false, false);

	if (this.ds_MenuKey.getRowCount() > 0) 
	{
		return true;
	}
	return false;
};

// Save processing
this.fn_Save = function ()
{
	return;
	// After validation process , if there not exception than , save the all modified data of Grid
	if (this.gfn_DsCheckValid(this.ds_Menu) == false) 
	{
		return;
	}

	this.gfn_AllRowDataCall(this.ds_Menu, this.fn_setSavedData);

	var sArgument = "";
	var sSvcID = "saveAll";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'";
	sArgument += " " + "method='saveAll'";
	sArgument += " " + "querySetCount='1'";
	sArgument += " " + "querySet1='createMenu,updateMenu,removeMenu'";

	var sInDatasets = "querySet1=ds_Menu:U";
	var sOutDatasets = "";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
};

this.fn_setSavedData = function (dsObj, nRow)
{
	dsObj.setColumn(nRow, "MENU_SEQ", nRow);

	var nTopRow = this.grd_menu.getTreeParentRow(nRow);

	if (nTopRow == -1) 
	{
		this.ds_Menu.setColumn(nRow, "MENU_GRP", this.ds_Menu.getColumn(nRow, "MENU_ID"));
		this.ds_Menu.setColumn(nRow, "MENU_TOP", "");
	}
	else 
	{
		this.ds_Menu.setColumn(nRow, "MENU_GRP", this.ds_Menu.getColumn(nTopRow, "MENU_GRP"));
		this.ds_Menu.setColumn(nRow, "MENU_TOP", this.ds_Menu.getColumn(nTopRow, "MENU_ID"));
		this.ds_Menu.setColumn(nRow, "MENU_LEVEL", this.ds_Menu.getColumn(nTopRow, "MENU_LEVEL") + 1);
	}
};


// Processing callback after processing gfnService 
this.fn_callback = function (strServiceId, strErrorCode, strErrorMsg)
{
	if (strErrorCode < 0) 
	{
		return this.gfn_Alert(strErrorMsg, "error");
	}

	switch (strServiceId) 
	{
		case "saveAll":
			this.gfn_Alert("msg.save.success");
			this.fn_Search();
			break;
		case "getListMenu":
			break;
		case "getCommCodeList":
			// Serch processing
			this.fn_Search();
			// Screen search processing
			this.fn_PageSearch();
			break;
		default:
			break;
	}
};
]]></Script>
