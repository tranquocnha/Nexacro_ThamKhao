// Menu search
this.fn_Search = function ()
{
	return;
	var sArgument = "";
	var sSvcID = "getListMenu";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getList'" + " ";
	sArgument += "GROUP_ID=" + TOBE.wrapQuote(this.cmb_Group.value) + " ";
	sArgument += "querySetCount='1'" + " ";
	sArgument += "querySet1='findGrouMenuList'";

	var sInDatasets = "";
	var sOutDatasets = "ds_Menu=querySet1";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
}


// Group search
this.fn_GroupSearch = function ()
{
	return;
	var sArgument = "";
	var sSvcID = "getListGroup";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'" + " ";
	sArgument += "method='getList'" + " ";
	sArgument += "querySetCount='1'" + " ";
	sArgument += "querySet1='findGroupList'";

	var sInDatasets = "";
	var sOutDatasets = "ds_Group=querySet1";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
}

// Save processing
this.fn_Save = function ()
{
	return;
	this.ds_Menu_svc.assign(this.ds_Menu);

	this.fn_setSavedData();

	var sArgument = "";
	var sSvcID = "saveAll";
	var sController = "svcurl::xpController.do";
	sArgument += "service='xpService'";
	sArgument += " " + "method='saveAll'";
	sArgument += " " + "querySetCount='1'";
	sArgument += " " + "querySet1='updateGroupMenu,updateGroupMenu,removeGroupMenu'";

	var sInDatasets = "querySet1=ds_Menu_svc:U";
	var sOutDatasets = "";
	var sCallbackFunc = "fn_callback";

	this.gfn_Transaction(sSvcID, sController, sInDatasets, sOutDatasets, sArgument, sCallbackFunc);
}

this.fn_setSavedData = function ()
{
	for (var i = 0; i < this.ds_Menu_svc.getRowCount(); i++) 
	{
		if (this.ds_Menu_svc.getRowType(i) == 2 || this.ds_Menu_svc.getRowType(i) == 4) 
		{
			if (this.ds_Menu_svc.getColumn(i, "USE_FG") == "0") 
			{
				this.ds_Menu_svc.deleteRow(i);
				i;
			}
		}
	}
}

// After processing gfnService process callback 
this.fn_callback = function (strServiceId, strErrorCode, strErrorMsg)
{
	if (strErrorCode < 0) 
	{
		return this.gfn_Alert(strErrorMsg, "error");
	}

	switch (strServiceId) 
	{
		case "saveAll":
			this.gfn_Alert("msg.save.success");
			this.fn_Search();
			break;
		case "getListGroup":
			if (this.ds_Group.rowcount == 0) 
			{
				return;
			}
			this.cmb_Group.set_index(0);
			this.fn_Search();
			break;
		default:
			break;
	}
}
