// Processing on Form beforeclose Event
// Before closing the form, here is pop-up creation to ask if there is modified data, pop-up creating
this.form_onbeforeclose = function (obj:Form, e:CloseEventInfo)
{
	if (this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		var retValue = this.gfn_Confirm("msg.before.movepage");
		if (retValue == false) 
		{
			return true;
		}

		this.fn_Save();
		return false;
	}
}

// Event processsing on save button click
this.btn_Save_onclick = function (obj:Button, e:ClickEventInfo)
{
	return;
	if (!this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		this.gfn_Alert("msg.save.nodata");
		return;
	}

	var retValue = this.gfn_Confirm("msg.before.save");
	if (retValue == false) 
	{
		return;
	}

	this.fn_Save();
}

// Event processing before changing group combo
this.cmb_Group_canitemchange = function (obj:Combo, e:ItemChangeEventInfo)
{
	if (this.gfn_DsGetUpdated(this.ds_Menu)) 
	{
		var retValue = this.gfn_Confirm("msg.before.search");
		if (retValue == false) 
		{
			return false;
		}
		return true;
	}
	return true;
}

// Event processing after changing group combo
this.cmb_Group_onitemchanged = function (obj:Combo, e:ItemChangeEventInfo)
{
	this.fn_Search();
}

// Event processing after chaging menu
this.ds_Menu_oncolumnchanged = function (obj:Dataset, e:DSColChangeEventInfo)
{
	var nRow;
	var i;

	var nLevel = obj.getColumn(e.row, "MENU_LEVEL");
	var strMenu = obj.getColumn(e.row, "MENU_ID");
	var strMenuTop = obj.getColumn(e.row, "MENU_TOP");
	var strMenuGrp = obj.getColumn(e.row, "MENU_GRP");

	for (i = e.row + 1; i < obj.getRowCount(); i++) 
	{
		if (nLevel >= obj.getColumn(i, "MENU_LEVEL")) 
		{
			break;
		}
		obj.setColumn(i, e.columnid, e.newvalue);
	}

	nRow = obj.findRow("MENU_ID", strMenuTop);
	while (nRow > -1) 
	{
		if (obj.getColumn(nRow, e.columnid) != e.newvalue) 
		{
			if (obj.findRowExpr("MENU_TOP=='" + strMenuTop + "'&&" + e.columnid + "!='" + e.newvalue + "'") > -1) 
			{
				break;
			}
			obj.setColumn(nRow, e.columnid, e.newvalue);
		}
		strMenuTop = obj.getColumn(nRow, "MENU_TOP");
		nRow = obj.findRow("MENU_ID", strMenuTop);
	}

