        <Static id="Static01" taborder="8" text="Loal file name" left="30" top="568" height="34" width="110"/>
        <Edit id="edt_LocalFileName" taborder="9" left="152" top="568" height="34" value="abc.txt" width="245" leftbase="" topbase="" bottombase="" rightbase="" widthbase="" heightbase="" border="1 solid #808080"/>
        <Button id="btn_down" taborder="10" text="Download" left="407" top="524" height="78" onclick="btn_down_onclick" width="152" background=""/>
